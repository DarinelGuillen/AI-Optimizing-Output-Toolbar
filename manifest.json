{
  "manifest_version": 3,
  "name": "Navbar GPT Promps",
  "description": "Provides a quick link to the G Suite Certification website",
  "version": "1.1",
  "icons": {
    "128": "img/sushi.png"
  },
  "action": {
    "default_icon": "img/sushi.png",
    "default_popup": "popup.html",
    "default_title": "G Suite Certification"
  },
  "permissions": [
    "storage",
    "notifications",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["styles/navbar.css", "navbar.html", "scripts/navbarActions.js", "styles/tailwind.css"],
      "matches": ["<all_urls>"]
    }
  ]

}
