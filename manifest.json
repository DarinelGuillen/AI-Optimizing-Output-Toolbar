{
  "manifest_version": 3,
  "name": "Navbar GPT Promps",
  "description": "Provides a quick link to the G Suite Certification website",
  "version": "1.1",
  "icons": {
    "128": "src/img/sushi.png"
  },
  "action": {
    "default_icon": "src/img/sushi.png",
    "default_popup": "src/popup.html",
    "default_title": "G Suite Certification"
  },
  "permissions": [
    "storage",
    "notifications",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/scripts/contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["src/styles/navbar.css", "src/navbar.html", "src/scripts/navbarActions.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
